<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Tenant.TenantViewRequestedRenterFragment">

    <TableLayout
        android:id="@+id/toplayout"
        android:layout_width="207dp"
        android:layout_height="141dp"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.07">

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView4"
                android:layout_width="107dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/assistant_extrabold"
                android:text="First Name" />

            <TextView
                android:id="@+id/view_renterProfile_firstN"
                android:layout_width="96dp"
                android:layout_height="wrap_content"
                android:fontFamily="@font/assistant_semibold"
                android:text="TextView" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/assistant_extrabold"
                android:text="Last Name" />

            <TextView
                android:id="@+id/view_renterProfile_lastN"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/assistant_semibold"
                android:text="TextView" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView14"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/assistant_extrabold"
                android:text="Role" />

            <TextView
                android:id="@+id/view_renterProfile_role"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/assistant_semibold"
                android:text="TextView" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView16"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/assistant_extrabold"
                android:text="Phone Number" />

            <TextView
                android:id="@+id/view_renterProfile_phoneNum"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/assistant_semibold"
                android:text="TextView" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView20"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/assistant_extrabold"
                android:text="Gender" />

            <TextView
                android:id="@+id/view_renterProfile_gender"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/assistant_semibold"
                android:text="TextView" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView21"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/assistant_extrabold"
                android:text="Birth Date" />

            <TextView
                android:id="@+id/view_renterProfile_born"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/assistant_semibold"
                android:text="TextView" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <TextView
                android:id="@+id/textView22"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/assistant_extrabold"
                android:text="City" />

            <TextView
                android:id="@+id/view_renterProfile_city"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:fontFamily="@font/assistant_semibold"
                android:text="TextView" />

        </TableRow>
    </TableLayout>

    <TableLayout
        android:id="@+id/bottomlayout"
        android:layout_width="304dp"
        android:layout_height="252dp"
        android:layout_marginStart="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toplayout">


        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:id="@+id/textView23"
                android:layout_width="290dp"
                android:layout_height="match_parent"
                android:fontFamily="@font/assistant_extrabold"
                android:text="Details On Me :)" />

        </TableRow>

        <TableRow
            android:layout_width="match_parent"
            android:layout_height="match_parent" >

            <TextView
                android:id="@+id/view_renterProfile_onMe"
                android:layout_width="294dp"
                android:fontFamily="@font/assistant_semibold"
                android:layout_height="231dp" />

        </TableRow>
    </TableLayout>

    <ImageView
        android:id="@+id/view_renterProfile_AcceptBtn"
        android:layout_width="138dp"
        android:layout_height="67dp"
        android:layout_marginStart="50dp"
        android:layout_marginTop="8dp"
        android:src="@drawable/accept_button_tenantviewrequested"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/bottomlayout"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/view_renterProfile_RejectBtn"
        android:layout_width="165dp"
        android:layout_height="72dp"
        android:layout_marginTop="2dp"
        android:src="@drawable/reject_button_tenantviewrequestedrenter"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/view_renterProfile_AcceptBtn"
        app:layout_constraintTop_toBottomOf="@+id/bottomlayout"
        tools:ignore="MissingConstraints" />

    <ListView
        android:id="@+id/view_renter_reviews"
        android:layout_width="409dp"
        android:layout_height="120dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toBottomOf="@+id/view_renterProfile_AcceptBtn"
        tools:layout_editor_absoluteX="1dp" />

</androidx.constraintlayout.widget.ConstraintLayout>